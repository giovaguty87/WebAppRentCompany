@model IEnumerable<WebAppRentCompany.Models.Office>

@{
    ViewBag.Title = "Index";
}

<h2>Offices</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id_city)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Postal_code)
        </th>
        <th>
